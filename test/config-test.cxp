            <xsl:value-of select="concat('FILE',$str_sep,$str_name_file,$str_sep,file[@name=$str_name_file]/@size,$str_sep,$int_min,$str_sep,'ERROR',$str_sep,'SIZE',$str_sep,$str_tip,$newline)"/>
